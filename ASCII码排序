#include <stdio.h>
int main()
{
	int n;
	char a,b,c,d;
	scanf("%d\n",&n);
	while(n-->0)
	{
		scanf(" %c%c%c",&a,&b,&c);
		if(a>=b)// 三个数字乱序排法
		{
			d=a;
			a=b;
			b=d;
		}
		if(a>=c)
		{
			d=a;
			a=c;
			c=d;
		}
		if(b>=c)
		{
			d=b;
			b=c;
			c=d;	
		}
		printf("%c %c %c\n",a,b,c);
	}
return 0;
}
